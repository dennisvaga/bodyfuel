# AWS Configuration
AWS_REGION=your-aws-region
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_S3_BUCKET_NAME=your-s3-bucket-name
AWS_ENDPOINT_URL=https://s3.your-region.amazonaws.com
AWS_BUCKET_ENDPOINT_URL=https://your-bucket-name.s3.your-region.amazonaws.com

# Authentication
AUTH_SECRET="your-auth-secret"

# Environment
NODE_ENV=production

# AI Provider API Keys
DEEPSEEK_API=your-deepseek-api-key

# Database
DATABASE_URL=postgresql://username:password@host:port/database_name

# Origins
ALLOWED_ORIGINS=https://your-allowed-origin.com,https://another-allowed-origin.com
